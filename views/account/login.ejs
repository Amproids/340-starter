<h1><%- title %></h1>
<%- messages() %>
<form id="loginForm">
    <fieldset>
        <label>Email:</label>
        <input type="email" name="email">
        <label>Password:</label>
        <input type="password" name="password">
        <button type="submit">Login</button>
        <p>No account? </p><a href="/account/register">Sign-up</a>
    </fieldset>
</form>